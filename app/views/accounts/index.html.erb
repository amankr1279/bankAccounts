<div class="container m-5">
<p style="color: green"><%= notice %></p>

<h1>Accounts</h1>
<br>
<div id="accounts">
  <% @accounts.each do |account| %>
      <% if account.user == current_user %>
      <%= render account %>
      <p>
        <%= link_to "Show this account", account %>
      </p>
      <% end %>
    <% end %>
</div>

<%= link_to "New account", new_account_path %>

<%= link_to "Home Page", root_path %>

</div>